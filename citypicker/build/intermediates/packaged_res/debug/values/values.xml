    <dimen name="cp_section_text_size">15sp</dimen>
    <string name="app_name">CityPicker</string>
    <string name="city_select_str">城市选择</string>
    <string name="cp_cancel">取消</string>
    <string name="cp_locate_failed">定位失败</string>
    <string name="cp_locating">正在定位…</string>
    <string name="cp_no_result">没有找到，换个词试试~</string>
    <string name="cp_search_hint_text">城市中文名或拼音</string>
    <string name="his_city_first">历</string>
    <string name="his_city_left_nav">历史</string>
    <string name="his_city_nav">历史城市</string>
    <string name="hot_city_first">热</string>
    <string name="hot_city_left_nav">热门</string>
    <string name="hot_city_nav">热门城市</string>
    <string name="loc_city_first">定</string>
    <string name="unknow">未知</string>
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/cp_colorPrimary</item>
        <item name="colorPrimaryDark">@color/cp_colorPrimaryDark</item>
        <item name="colorAccent">@color/cp_colorAccent</item>
    </style>
    <style name="CityPickerStyle" parent="AlertDialog.AppCompat">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="DefaultCityPickerAnimation" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/cp_push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/cp_push_bottom_out</item>
    </style>
    <style name="DefaultCityPickerTheme" parent="AppTheme">
        <!--        主页背景颜色-->
        <item name="cpMainBgColor">@color/white</item>
        <!--        搜索主背景颜色-->
        <item name="cpMainSearchBgColor">@color/white</item>
        <item name="cpSearchItemBackground">@color/white</item>
        <item name="cpSearchItemHeight">@dimen/cp_section_height</item>
        <item name="cpSearchItemTextColor">@color/cp_color_gray_dark</item>
        <item name="cpSearchItemTextSize">@dimen/cp_search_text_size</item>
        <item name="cpSearchItemHorizontal">@dimen/cp_default_padding</item>
        <item name="cpSearchHeight">@dimen/cp_empty_icon_height</item>
        <item name="cpSearchPaddingHo">15dp</item>
        <item name="cpSearchLeft">@null</item>
        <item name="cpSearchLeftPadding">34dp</item>
        <item name="cpSearchClearRight">14dp</item>
<!--        搜索找到的子-->
        <item name="cpSearchColor">@color/cp_color_gray_dark</item>

<!--        标题-->
        <item name="cpTitleHeight">50dp</item>
        <item name="cpTitleLeft">@null</item>
        <item name="cpTitleStr">@string/city_select_str</item>
        <item name="cpTitleTextSize">15sp</item>
        <item name="cpTitleTextColor">@color/cp_color_gray_dark</item>
        <!--        搜索框点击前和点击后的背景-->
        <item name="cpSearchBg1">#00000000</item>
        <item name="cpSearchBg2">#00000000</item>
        <!--        取消按钮-->
        <item name="cpCancelTextSize">@dimen/cp_cancel_text_size</item>
        <item name="cpCancelTextColor">@color/cp_color_gray</item>

        <!--        搜索-->
        <item name="cpClearTextIcon">@mipmap/cp_icon_clear_all</item>
        <item name="cpSearchTextSize">@dimen/cp_search_text_size</item>
        <item name="cpSearchTextColor">@color/cp_color_gray_dark</item>
        <item name="cpSearchHintText">@string/cp_search_hint_text</item>
        <item name="cpSearchHintTextColor">@color/cp_color_gray_light</item>
        <item name="cpSearchCursorDrawable">@color/cp_color_gray_dark</item>

        <!--        搜索为空-->
        <item name="cpEmptyIcon">@mipmap/cp_icon_empty</item>
        <item name="cpEmptyIconWidth">@dimen/cp_empty_icon_width</item>
        <item name="cpEmptyIconHeight">@dimen/cp_empty_icon_height</item>
        <item name="cpEmptyText">@string/cp_no_result</item>
        <item name="cpEmptyTextSize">@dimen/cp_empty_text_size</item>
        <item name="cpEmptyTextColor">@color/cp_color_gray_deep</item>

        <!--        定位城市item-->
        <item name="cpGridLocationItemBackground">@drawable/cp_grid_item_bg</item>
        <item name="cpListItemTextSize">@dimen/cp_list_item_text_size</item>
        <item name="cpListItemTextColor">@color/cp_color_gray_deep</item>

        <!--        历史，热门item-->
        <item name="cpGridItemBackground">@drawable/cp_grid_item_bg</item>
        <item name="cpGridItemSpace">@dimen/cp_grid_item_space</item>
        <item name="cpListHisHotItemTextSize">@dimen/cp_list_item_text_size</item>
        <item name="cpListHisHotItemTextColor">@color/cp_color_gray_deep</item>

        <!--    item-->
        <item name="cpListItemHeight">@dimen/cp_list_item_height</item>
        <item name="cpListLocationItemTextSize">@dimen/cp_list_item_text_size</item>
        <item name="cpListLocationItemTextColor">@color/cp_color_gray_deep</item>

        <!--        顶部标签1-->
        <item name="cpSectionHeight1">@dimen/cp_section_height</item>
        <item name="cpSectionTextSize1">@dimen/cp_section_text_size</item>
        <item name="cpSectionTextColor1">@color/cp_color_gray</item>
        <item name="cpSectionBackground1">@color/cp_color_section_bg</item>

        <!--        顶部标签2-->
        <item name="cpSectionHeight2">@dimen/cp_section_height</item>
        <item name="cpSectionTextSize2">@dimen/cp_section_text_size</item>
        <item name="cpSectionTextColor2">@color/cp_color_gray</item>
        <item name="cpSectionBackground2">@color/cp_color_section_bg</item>

        <!--        右边字母-->
        <item name="cpIndexBarTextSize">@dimen/cp_index_bar_text_size</item>
        <item name="cpIndexBarNormalTextColor">@color/cp_color_gray</item>
        <item name="cpIndexBarSelectedTextColor">@color/cp_color_gray_dark</item>
        <item name="cpIndexBarMarginTop">0dp</item>
        <item name="cpIndexBarMarginBottom">0dp</item>

        <!--        中间tab显示-->
        <item name="cpOverlayWidth">@dimen/cp_overlay_width</item>
        <item name="cpOverlayHeight">@dimen/cp_overlay_height</item>
        <item name="cpOverlayTextSize">@dimen/cp_overlay_text_size</item>
        <item name="cpOverlayTextColor">@android:color/white</item>
        <item name="cpOverlayBackground">@drawable/cp_overlay_bg</item>
    </style>
</resources>